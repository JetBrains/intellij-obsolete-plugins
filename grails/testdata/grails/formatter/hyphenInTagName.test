<%@ page contentType="text/html;charset=UTF-8" %>
<content tag="content">

  <fb:js-string var="dialogText">
    <g:form action="addProfile">
      <g:textField name="athleteQuery" id="searchAthlete" size="25" autocomplete="off"/>
      <div id="athleteRace"></div>
      <g:hiddenField name="athlete" id="selectedAthlete"/>
    </g:form>
  </fb:js-string>
  <fb:js-stringvar="dialogText">
  </fb:js-string>

</content>
