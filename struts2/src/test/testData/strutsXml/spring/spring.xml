<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="bean1" class="MyClass"/>
  <alias name="bean1" alias="bean1Alias"/>

  <bean name="bean2" class="MyClass"/>

  <bean name="abstractBean" class="MyClass" abstract="true"/>

  <!-- interceptor -->
  <bean name="springInterceptor" class="com.opensymphony.xwork2.spring.interceptor.ActionAutowiringInterceptor"/>

  <!-- result-type -->
  <bean name="springResultType" class="com.opensymphony.xwork2.ActionChainResult"/>

</beans>                        